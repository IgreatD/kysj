{
  "name": "ms-sanyi",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "bootstrap": "yarn || npm i",
    "new": "node ./build/bin/new.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:report": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --reporters=default --reporters=jest-junit",
    "docker-hub:build": "docker build  -f Dockerfile.hub -t  sanyi-admin ./",
    "docker-prod:build": "docker-compose -f ./docker/docker-compose.yml build",
    "docker-prod:up": "docker-compose -f ./docker/docker-compose.yml up",
    "docker:push": "npm run docker-hub:build && npm run docker:tag && docker push igreatd/sanyi-admin",
    "docker:tag": "docker tag sanyi-admin igreatd/sanyi-admin",
    "pub": "yarn bootstrap && sh ./build/git-release.sh && sh ./build/deploy.sh"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cnchar": "^1.0.5",
    "core-js": "^2.6.5",
    "cos-js-sdk-v5": "^0.5.20",
