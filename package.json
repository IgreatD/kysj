    "@types/jest": "^23.1.4",
    "@types/lodash": "^4.14.136",
    "@types/nprogress": "^0.2.0",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
