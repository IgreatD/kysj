    "file-saver": "^2.0.2",
    "fuse.js": "^3.4.5",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^3.0.0",
    "qrcodejs2": "^0.0.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "8.2.2",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.9.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
